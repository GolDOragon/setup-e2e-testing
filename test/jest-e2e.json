{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "globalSetup": "./globalSetup.ts",
  "globalTeardown": "./globalTeardown.ts",
  "setupFilesAfterEnv": ["./setup.ts"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  }
}
